#OSS WLAN setup
service macaddrsetup /system/bin/macaddrsetup /sys/devices/fb000000.qcom,wcnss-wlan/wcnss_mac_addr
    class main
    user root
    oneshot

#QCOM sensors
service sensors /system/vendor/bin/sensors.qcom
    class main
    user root
    group root

# Copy Modem blobs
service modem_copy /system/bin/sh /init.modem_copy.sh
    class core
    user root
    group root
    oneshot
